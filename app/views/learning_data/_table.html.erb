  <% learning_data.each do |data| %>
  <tbody>
    <tr>
      <td><div class='ms-3 mt-2'><%= data.learning_subject %></div></td>
      <td class='number'><input type="number" id="<%= data.id %>" value=<%= data.time %> style='margin-top: 7px;'></td>
      <td><div class='btn-border btn btn-light'>学習時間を保存する</div></td>
      <td class='text-center'><div class='btn-red btn btn-danger'>削除する</div></td>
    </tr>
  </tbody>
  <% end %>
